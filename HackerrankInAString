char* hackerrankInString(char* s) {
    char *a = "hackerrank";
    int count=0, k=0;
    
    for(int i=0; i<strlen(s); i++)
    {
        if(s[i]== a[k])
        {
            k++;
            count++;
        }
    }
    if(count == strlen(a)) 
    {
        return "YES";
    }
    else return "NO";
}
